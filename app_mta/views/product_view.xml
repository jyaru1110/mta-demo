<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record model="ir.ui.view" id="product_view_list">
            <field name="name">product.template.list</field>
            <field name="model">product.template</field>
            <field name="arch" type="xml">
                <tree string="Product List">
                    <field name="name"/>
                    <field name="default_code"/>
                    <field name="buffer_size"/>
                    <field name="oc"/>
                    <field name="bp_solicitud"/>
                    <field name="qty_transit"/>
                    <field name="bp_transito"/>
                    <field name="qty_available"/>
                    <field name="bp_disponible"/>
                </tree>
            </field>
        </record>
    </data>
</odoo>