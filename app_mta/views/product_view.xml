<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record model="ir.ui.view" id="producto_mta_list">
            <field name="name">producto.mta.list</field>
            <field name="model">mta.producto</field>
            <field name="arch" type="xml">
                <tree string="Product list">
                    <field name="name"/>
                    <field name="default_code"/>
                    <field name="buffer_size"/>
                    <field name="oc"/>
                    <field name="bp_solicitud" class="condition" />
                    <field name="qty_transit" />
                    <field name="bp_transito" class="condition" />
                    <field name="qty_sitio" />
                    <field name="bp_disponible" />
                    <field name="alerta"/>
                </tree>
            </field>
        </record>
    </data>
</odoo>