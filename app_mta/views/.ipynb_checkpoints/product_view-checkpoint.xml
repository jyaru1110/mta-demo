<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record model="ir.ui.view" id="producto_mta_list">
            <field name="name">producto.mta.list</field>
            <field name="model">mta.producto</field>
            <field name="arch" type="xml">
                <tree string="Product list">
                    <field name="name"/>
                    <field name="default_code"/>
                    <field name="lt"/>
                    <field name="loteOptimo"/>
                    <field name="buffer_size"/>
                    <field name="oc"/>
                    <field name="bp_solicitud" widget="progress_bar_widget"/>
                    <field name="qty_transit"/>
                    <field name="bp_transito" widget="progress_bar_widget"/>
                    <field name="qty_available"/>
                    <field name="bp_sitio" widget="progress_bar_widget"/>
                    <field name="alerta"/>
                </tree>
            </field>
        </record>
        <record model="ir.ui.view" id="producto_mta_form">
            <field name="name">producto.mta.form</field>
            <field name="model">mta.producto</field>
            <field name="arch" type="xml">
                <form string="TEST">
                    <group>
                        <field name="buffer_size" widget="chart_widget"/>
                        <field name="product_tmpl_id"/>
                    </group>
                </form>
            </field>
        </record>
    </data>
</odoo>