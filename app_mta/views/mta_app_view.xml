<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record model="ir.actions.act_window" id="product_mta_list_action">
            <field name="name">Producto</field>
            <field name="res_model">mta.producto</field>
            <field name="view_mode">tree, form</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    Manage inventory
                </p>
            </field>
        </record>
        
        <menuitem name="MTA APP"
                  id="menu_mta_app"
                  sequence="1"/>
        <menuitem id="menu_product_sub" name="Products" parent="menu_mta_app" sequence="2"/>
        <menuitem id="menu_products" name="Products" parent="menu_product_sub" sequence="3"
                  action="product_mta_list_action"/>
        <menuitem id="menu_js" name="Javascript" parent="menu_product_sub" sequence="3"
                  action="product_mta_list_action"/>
    </data>
</odoo>  
